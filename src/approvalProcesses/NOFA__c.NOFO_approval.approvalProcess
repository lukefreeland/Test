        </criteriaItems>
        <criteriaItems>
            <field>NOFA__c.Corporate_Program_Code__c</field>
            <operation>notEqual</operation>
            <value>null</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
