    <webLinks>
        <fullName>Eligible</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Eligible</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 
var c = new sforce.SObject(&quot;Save_the_Date__c&quot;); 
c.ID=&quot;{!Save_the_Date__c.Id}&quot;;
var process = sforce.connection.query(&quot;Select Id,Response__c,Status__c from Save_the_Date__c where id=&apos;{!Save_the_Date__c.Id}&apos;&quot;); 
customrecord = process.getArray(&quot;records&quot;);
var rec = customrecord[0].Response__c;
alert(rec);
if(rec==&quot;Available&quot;)
{
alert(&apos;Eligible&apos;);
customrecord[0].Status__c = &quot;Eligible&quot;;
}

else if(rec==&quot;Available w/Comments&quot;)
{
alert(&apos;Eligible-COI&apos;);
customrecord[0].Status__c = &quot;Eligible - COI&quot;;
}
else {
 alert(&apos;Only Available records are eligible&apos;);
}
sforce.connection.update([customrecord[0]]);
location.reload(true);</url>
