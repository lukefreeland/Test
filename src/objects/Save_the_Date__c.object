        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/33.0/apex.js&quot;)} 
var c = new sforce.SObject(&quot;Save_the_Date__c&quot;); 
c.ID=&quot;{!Save_the_Date__c.Id}&quot;;
var process = sforce.connection.query(&quot;Select Id,Status__c,Response__c from Save_the_Date__c where id=&apos;{!Save_the_Date__c.Id}&apos;&quot;); 
customrecord = process.getArray(&quot;records&quot;);
var rec = customrecord[0].Response__c; 
if(rec == &quot;Available&quot;)
{
customrecord[0].Status__c = &quot;InEligible&quot;;
}
else if(rec==&quot;Available w/Comments&quot;) 
{ 
customrecord[0].Status__c = &quot;InEligible&quot;; 
} 
else 
{
alert(&apos;Only Available records can become InEligible&apos;);
}
result = sforce.connection.update([customrecord[0]]);
location.reload(true);</url>
