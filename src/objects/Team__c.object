    <fields>
        <fullName>Active1__c</fullName>
        <description>Active field on the Team object should be populated with ‘Yes’ if the Effective Date is less than or equal to the current date, and the End Date is NULL, or the End Date is greater than or equal to today. It should be populated with ‘No’ if the Effective Date is less than the current date and the End Date is ‘Null”, or if the End Date is less than the current date.</description>
        <externalId>false</externalId>
        <formula>IF (OR(AND(Effective_Date__c &lt;= TODAY(),ISNULL(End_Date__c)),AND(Effective_Date__c &lt;= TODAY(),End_Date__c &gt;= today() )) ,&quot;Yes&quot;, 
IF(OR(AND(Effective_Date__c &gt; TODAY(),ISNULL(End_Date__c)),(End_Date__c &lt; today()),AND(Effective_Date__c &gt; TODAY(),End_Date__c &gt; today())),&quot;No&quot;,&quot;&quot;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Active</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
