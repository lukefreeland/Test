    <validationRules>
        <fullName>Number_Validation_On_Report_Number</fullName>
        <active>true</active>
        <description>number validation separated by hyphen</description>
        <errorConditionFormula>NOT(
OR(
LEN (Report_Number__c) = 0,
REGEX(Report_Number__c, &quot;[0-9]{4}-[0-9]{2}&quot;)
))</errorConditionFormula>
        <errorDisplayField>Report_Number__c</errorDisplayField>
        <errorMessage>Report Number should require 6 numbers be entered and formatted as ####-##, e.g. 1234-56</errorMessage>
