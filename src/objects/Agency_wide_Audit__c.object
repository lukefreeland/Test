    <sharingModel>Private</sharingModel>
    <validationRules>
        <fullName>Actions_Taken_to_Address_Going_Concern</fullName>
        <active>true</active>
        <errorConditionFormula>AND(Going_Concern__c = False , LEN(Actions_Taken_to_Address_Going_Concern__c) &lt;&gt; 0 ,  RecordType.Name = &apos;Single Audit&apos; )</errorConditionFormula>
        <errorDisplayField>Actions_Taken_to_Address_Going_Concern__c</errorDisplayField>
        <errorMessage>Actions Taken to Address Going Concern” can only be entered if the “Going Concerns” checkbox is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CNCS_Management_Decision_validation</fullName>
        <active>true</active>
        <description>If &apos;Mark as final?&apos; is unchecked the CNCS Management decision field should be available</description>
        <errorConditionFormula>AND
(LEN(CNCS_Management_Decision__c) &lt;&gt;0,
Mark_as_final__c = TRUE ,RecordType.Name = &apos;Agency-wide Audit&apos;
)</errorConditionFormula>
        <errorDisplayField>CNCS_Management_Decision__c</errorDisplayField>
        <errorMessage>CNCS Management Decision cannot be entered if &quot;Mark as Final?&quot; is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Audit_Closed</fullName>
        <active>true</active>
        <errorConditionFormula>AND( PAR_Corrective_Action_Complete__c = False, NOT((ISNULL( Date_Audit_Closed__c ))), RecordType.Name=&quot;Single Audit&quot;)</errorConditionFormula>
        <errorDisplayField>Date_the_Audit_is_Closed__c</errorDisplayField>
        <errorMessage>Date Audit Closed cannot be entered if Corrective Action Complete is null.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Auditee_30_Day_Response_to_Draft</fullName>
        <active>true</active>
        <errorConditionFormula>Date_Auditee_30_Day_Response_to_Draft__c &lt;=  Date_Draft_OIG_Report_Issued__c</errorConditionFormula>
        <errorDisplayField>Date_Auditee_30_Day_Response_to_Draft__c</errorDisplayField>
        <errorMessage>The Date Auditee 30 Day Response to Draft OIG Report Issued must be greater than Date Draft Report Issued.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_CNCS_30_Day_Response_to_Draft_OIG</fullName>
        <active>true</active>
        <errorConditionFormula>Date_CNCS_30_Day_Response_to_Draft_OIG__c   &lt;=  Date_Draft_Report_Issued__c</errorConditionFormula>
        <errorDisplayField>Date_CNCS_30_Day_Response_to_Draft_OIG__c</errorDisplayField>
        <errorMessage>The Date CNCS 30 Day Response to Draft OIG Report Issued must be greater than Date Draft Report Issued.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_CNCS_Responded_to_Draft_validation</fullName>
        <active>true</active>
        <description>The Date CNCS Responded to Draft Report must be greater than or equal to the Date Draft Report Issued.</description>
        <errorConditionFormula>AND( Date_CNCS_Responded_to_Draft_Report__c &lt;  Date_Draft_Report_Issued__c )</errorConditionFormula>
        <errorDisplayField>Date_CNCS_Responded_to_Draft_Report__c</errorDisplayField>
        <errorMessage>The Date CNCS Responded to Draft Report must be greater than or equal to the Date Draft Report Issued.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Draft_OIG_Report_Issued</fullName>
        <active>true</active>
        <errorConditionFormula>Date_Draft_OIG_Report_Issued__c  &lt;  OIG_Exit_Conference_Date__c</errorConditionFormula>
        <errorDisplayField>Date_Draft_OIG_Report_Issued__c</errorDisplayField>
        <errorMessage>The Date Draft OIG Report Issued must be greater than or equal to the OIG Exit Conference Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Draft_Report_validation</fullName>
        <active>true</active>
        <description>The Date Draft Report Issued must be greater than or equal to the Exit Conference Date.</description>
        <errorConditionFormula>AND( Date_Draft_Report_Issued__c &lt; Exit_Conference_Date__c )</errorConditionFormula>
        <errorDisplayField>Date_Draft_Report_Issued__c</errorDisplayField>
        <errorMessage>The Date Draft Report Issued must be greater than or equal to the Exit Conference Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Final_OIG_Report_Issued</fullName>
        <active>true</active>
        <errorConditionFormula>Date_Final_OIG_Report_Issued__c &lt; Date_Draft_OIG_Report_Issued__c</errorConditionFormula>
        <errorDisplayField>Date_Final_OIG_Report_Issued__c</errorDisplayField>
        <errorMessage>The Date Final OIG Report Issued must be greater than or equal to the Date Draft OIG Report Issued.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Management_Decision_sent_to_OIG</fullName>
        <active>true</active>
        <description>Date Management Decision sent to OIG can only be entered if the Proposed Management Decision sent to OIG checkbox is checked.</description>
        <errorConditionFormula>AND(  Proposed_Management_Decision_sent_to_OIG__c  = False, RecordType.Name = &apos;Agency-wide Audit&apos;,NOT(ISNULL( Date_Management_Decision_sent_to_OIG__c )))</errorConditionFormula>
        <errorDisplayField>Date_Management_Decision_sent_to_OIG__c</errorDisplayField>
        <errorMessage>Date Management Decision sent to OIG can only be entered if the Proposed Management Decision sent to OIG checkbox is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Notice_of_Final_Action_Issued</fullName>
        <active>true</active>
        <description>Date Notice of Final Action Issued can only be entered if the Proposed Management Decision sent to OIG checkbox is checked.</description>
        <errorConditionFormula>AND( Proposed_Management_Decision_sent_to_OIG__c = False, RecordType.Name = &apos;Agency-wide Audit&apos;,NOT(ISNULL( Date_Notice_of_Final_Action_Issued__c )))</errorConditionFormula>
        <errorDisplayField>Date_Notice_of_Final_Action_Issued__c</errorDisplayField>
        <errorMessage>Date Notice of Final Action Issued can only be entered if the Proposed Management Decision sent to OIG checkbox is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Date_Notice_of_Final_Action_validation</fullName>
        <active>true</active>
        <description>Date entered must be greater than or equal to Date Management Decision Sent to OIG</description>
        <errorConditionFormula>AND( Date_Notice_of_Final_Action_Issued__c &lt;  Date_Management_Decision_sent_to_OIG__c )</errorConditionFormula>
        <errorDisplayField>Date_Notice_of_Final_Action_Issued__c</errorDisplayField>
