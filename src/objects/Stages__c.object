    <validationRules>
        <fullName>EnforceDatesCheckbox</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISPICKVAL(Review_Incomplete_Action__c, &quot;--None--&quot;)), NOT(ISPICKVAL(Review_Incomplete_Action__c, &quot;--None--&quot;)),
NCCC_Enforce_Dates_for_Reviews__c = True,
NOT(ISBLANK( Stage_Group__c )),NOT(ISBLANK( Stage_Sequence__c )))</errorConditionFormula>
        <errorMessage>The Following fields can only be entered when the NCCC Enforce Dates Checkbox is checked.
Stage Group
Stage Sequence
Extension Date
Review Complete Action
Review Incomplete Action</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>ExtensionDate</fullName>
        <active>true</active>
        <errorConditionFormula>Extension_Date__c  &lt;  End_Date__c</errorConditionFormula>
        <errorDisplayField>Extension_Date__c</errorDisplayField>
        <errorMessage>The extension date must be greater than the “Stage End Date.”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Review_end_and_start_date</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c &gt;= End_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Review End Date must be greater than Review Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Weight_Score</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISNULL(Total_Weight__c  )) ,(Total_Weight__c &gt; 100))</errorConditionFormula>
        <errorDisplayField>Consensus_Approach__c</errorDisplayField>
        <errorMessage>‘Total weight of all questions in the stage cannot exceed 100.’</errorMessage>
    </validationRules>
