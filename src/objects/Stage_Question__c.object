    <validationRules>
        <fullName>Section_Selection</fullName>
        <active>true</active>
        <errorConditionFormula>OR(AND(Stages_ID__r.Section_1__c == Null,ISPICKVAL(Section__c , &apos;Section 1&apos;)),(AND(Stages_ID__r.Section_2__c == Null ,ISPICKVAL(Section__c , &apos;Section 2&apos;))),(AND(Stages_ID__r.Section_3__c == Null ,ISPICKVAL(Section__c , &apos;Section 3&apos;))),(AND(Stages_ID__r.Section_4__c == Null ,ISPICKVAL(Section__c , &apos;Section 4&apos;))), 
(AND(Stages_ID__r.Section_5__c ==Null ,ISPICKVAL(Section__c , &apos;Section 5&apos;))))</errorConditionFormula>
        <errorMessage>Selected Section doesnt have TITLE</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_Question_Active</fullName>
        <active>true</active>
