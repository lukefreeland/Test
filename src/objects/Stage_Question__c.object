    </validationRules>
    <validationRules>
        <fullName>Yes_comments_req_when_check_box_checked</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( Allow_Comments__c , ISBLANK(TEXT(If_Yes_Comments_Required__c)))</errorConditionFormula>
