    <webLinks>
        <fullName>New_Custom</fullName>
        <availability>online</availability>
        <description>Button to populate fields from Application/Concept Paper</description>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>false</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/34.0/connection.js&quot;)}

var myquery = &quot;SELECT Id, Name, RecordType.Name, App_State_Commission_Input_Deadline__c from Application3__c Where Id = &apos;{!Application3__c.Id}&apos; &quot;;


var result = sforce.connection.query(myquery);
var records = result.getArray(&quot;records&quot;); 

if(records[0])
{
 var app = records[0]; 
 var today = new Date();
 var dd = today.getDate();
 var mm = today.getMonth()+1; 
 var yyyy = today.getFullYear();
 if(dd&lt;10){
  dd=&apos;0&apos;+dd
 } 
 if(mm&lt;10){
  mm=&apos;0&apos;+mm
 } 
 var todaysDate = yyyy+&apos;-&apos;+mm+&apos;-&apos;+dd;


 if((app.App_State_Commission_Input_Deadline__c &gt; todaysDate)&amp;&amp; app.RecordType.Name == &apos;Application&apos;){
	if(&apos;{!$Profile.Name}&apos; == &apos;State Commission Profile&apos;){
	window.location.replace(&quot;/statecommission/a3Q/e?CF00Nr0000000Zz4n={!Application3__c.Name}&amp;CF00Nr0000000Zz4n_lkid = {!Application3__c.Id}&amp;retURL=%2F{!Application3__c.Id}&amp;{!State_Commission_Input__c.RecordTypeId} = &apos;012r00000004Skp&apos;&quot;);
	} 

	else {
	window.location.replace(&quot;/a3Q/e?CF00Nr0000000Zz4n={!Application3__c.Name}&amp;CF00Nr0000000Zz4n_lkid = {!Application3__c.Id}&amp;retURL=%2F{!Application3__c.Id}&amp;{!State_Commission_Input__c.RecordTypeId} = &apos;012r00000004Skp&apos;&quot;);
	}
  }
  else if((app.App_State_Commission_Input_Deadline__c &gt; todaysDate) &amp;&amp; app.RecordType.Name == &apos;Concept Paper&apos;){
	if(&apos;{!$Profile.Name}&apos; == &apos;State Commission Profile&apos;){
	window.location.replace(&quot;/statecommission/a3Q/e?CF00Nr0000000Zz4n={!Application3__c.Name}&amp;CF00Nr0000000Zz4n_lkid = {!Application3__c.Id}&amp;retURL=%2F{!Application3__c.Id}&amp;{!State_Commission_Input__c.RecordTypeId} = &apos;012r00000004Sku&apos;&quot;);
	} 
	else {
	window.location.replace(&quot;/a3Q/e?CF00Nr0000000Zz4n={!Application3__c.Name}&amp;CF00Nr0000000Zz4n_lkid = {!Application3__c.Id}&amp;retURL=%2F{!Application3__c.Id}&amp;{!State_Commission_Input__c.RecordTypeId} = &apos;012r00000004Sku&apos;&quot;);
	}
  }
 else{
   alert(&apos;State Commission Input cannot be created after the Deadline&apos;);
 }
}</url>
