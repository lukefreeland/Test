    <sharingModel>ReadWrite</sharingModel>
    <validationRules>
        <fullName>Close_Out_Certification_Complete</fullName>
        <active>false</active>
        <errorConditionFormula>IF(RecordType.DeveloperName == &apos;Subgrantee Certification&apos; &amp;&amp; Close_Out_Certification_Complete__c != True, True, False)</errorConditionFormula>
        <errorMessage>Close Out Certification Complete is required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Comments_Other_Issues</fullName>
        <active>true</active>
        <description>Fired when &quot;Drawdowns within 20% of reported OBG&quot; is NO, to prompt user to add comments.</description>
        <errorConditionFormula>IF(Drawdown_Amount__c&gt;(0.2*Reported_Obligations__c)&amp;&amp; Other_issues__c = NULL,True,False)</errorConditionFormula>
        <errorDisplayField>Other_issues__c</errorDisplayField>
        <errorMessage>Since &quot;Drawdowns&quot; are not within 20%, comments will need to be entered into the &quot;other issues&quot; text box.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Expenditure_Comments</fullName>
        <active>true</active>
        <errorConditionFormula>IF((ISPICKVAL(Unobligated_funds__c,&apos;Low&apos;)||ISPICKVAL(Unobligated_funds__c,&apos;High&apos;))&amp;&amp;ISBLANK(Expenditure_Comments__c),True,false)</errorConditionFormula>
        <errorDisplayField>Expenditure_Comments__c</errorDisplayField>
        <errorMessage>Enter &quot;Expenditures Comments&quot; as Unobligated funds are not on Target.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Federal_funds</fullName>
        <active>true</active>
        <description>As per #870</description>
        <errorConditionFormula>if((( of_funds_expended_to_date__c -  of_funds_expended_to_date_Federal__c ) != ( Federal_Share_of_expenditures__c / Total_Federal_Funds_authorized__c )), true, false)</errorConditionFormula>
        <errorMessage>% of Funds Expended to Date minus Federal Share must equal federal share of expenditures divided by total federal funds authorized</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Grantee_funds</fullName>
        <active>true</active>
        <description>As per #870</description>
        <errorConditionFormula>if(((  of_funds_expended_to_date_Federal__c -   of_funds_expended_to_date__c ) != (  Recipient_share_of_expenditures__c /  Total_Recipient_share_required__c )), true, false)</errorConditionFormula>
        <errorMessage>% of Funds expended to date minus grantee share must equal recipient share of expenditures divided by total recipient share required</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lock_the_record</fullName>
        <active>true</active>
        <description>Lock the record when the status is submitted</description>
        <errorConditionFormula>AND(ISPICKVAL(Status__c,&apos;Submitted&apos;),
 $Profile.Name = &apos;Applicant&apos;)</errorConditionFormula>
        <errorMessage>You cannot edit the record because the status is submitted.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Match_Comments</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISPICKVAL(Match__c,&apos;Low&apos;)&amp;&amp;ISBLANK(Match_Comments__c),True,false)</errorConditionFormula>
        <errorDisplayField>Match_Comments__c</errorDisplayField>
