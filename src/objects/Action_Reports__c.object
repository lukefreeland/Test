    </validationRules>
    <validationRules>
        <fullName>Due_Date_Greater_to_Reporting_Period_End</fullName>
        <active>true</active>
        <description>Due Date - This will be a date field that must be greater than &quot;Reporting Period End Date&quot; date.</description>
        <errorConditionFormula>Due_Date__c  &lt;  Reporting_Period_End_Date__c</errorConditionFormula>
        <errorDisplayField>Due_Date__c</errorDisplayField>
        <errorMessage>Due Date  must be greater than &quot;Reporting Period End Date&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_date_must_be_great_than_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Reporting_Period_End_Date__c  &lt; Reporting_Period_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Reporting_Period_End_Date__c</errorDisplayField>
        <errorMessage>Report Period Ending Date must be greater than &quot;Reporting Period Start Date&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Extended_due_date_greater_than_due_date</fullName>
        <active>true</active>
        <errorConditionFormula>Extended_Due_Date__c  &lt; Due_Date__c</errorConditionFormula>
        <errorDisplayField>Extended_Due_Date__c</errorDisplayField>
        <errorMessage>Extended Due Date must be greater than &quot;Due Date&quot;</errorMessage>
    </validationRules>
