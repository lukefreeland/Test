    <validationRules>
        <fullName>Action_Reports_Not_Editable</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL( NOFA_ID__r.Status__c , &apos;Review in Progress&apos;),NOT( ISNEW()))</errorConditionFormula>
