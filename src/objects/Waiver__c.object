    <validationRules>
        <fullName>Minimum_Required_Match</fullName>
        <active>true</active>
        <description>Minimum Required Match % required for all request types except AmeriCorps Alternative Match Schedule.</description>
        <errorConditionFormula>AND(
OR(
ISPICKVAL(Request_Type__c,&quot;Volunteer Leveraging Waiver&quot;),
ISPICKVAL(Request_Type__c,&quot;Commission Administrative Alternative Match Schedule&quot;),
ISPICKVAL(Request_Type__c, &quot;Severely Distressed Communities Match Waiver&quot;),
ISPICKVAL(Request_Type__c, &quot;Sub-grants Waiver&quot;),
ISPICKVAL(Request_Type__c, &quot;Other&quot;)
),
ISBLANK( Minimum_Required_Match__c )
)</errorConditionFormula>
        <errorDisplayField>Minimum_Required_Match__c</errorDisplayField>
        <errorMessage>The &quot;Minimum Required Match %&quot; must be entered for the selected request type.</errorMessage>
    </validationRules>
