    <webLinks>
        <fullName>Close_Grant</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Close Grant</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/apex.js&quot;)} 

var query = &quot;SELECT Id,Status__c,Close_Project__c,Grant_Close_Out_Date__c,PO_Name__c,PO_Certified_Date__c,Financial_Closeout_Date__c from Grant_Close_Out__c Where Id= &apos;{!Grant_Close_Out__c.Id}&apos; Limit 1&quot;; 

var result = sforce.connection.query(query); 

var records = result.getArray(&quot;records&quot;); 

var gc = records[0]; 
if(gc.PO_Name__c==null||gc.PO_Certified_Date__c==null){
window.alert(&apos;Grant cannot be closed because PO Certification is not complete. Please retry after PO Certification is performed.&apos;);
}else if(gc.Financial_Closeout_Date__c==null){
window.alert(&apos;Grant cannot be closed since Financial Closeout is not complete. Please retry after financial closeout is performed.&apos;);
}else{
gc.Status__c=&apos;Complete&apos;;
gc.Grant_Close_Out_Date__c=new Date();
}
var results = sforce.connection.update([records[0]]); 
window.location.reload();</url>
    </webLinks>
    <webLinks>
        <fullName>Perform_Financial_Close_Out</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Perform Financial Close Out</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/35.0/apex.js&quot;)} 

var query = &quot;SELECT Id,Name,Advanced_Momentum__c,Disbursed_Momentum_SF_425__c,Total_Federal_Expenditure_Final_FFR_SF__c from Grant_Close_Out__c Where Id= &apos;{!Grant_Close_Out__c.Id}&apos; Limit 1&quot;; 

var result = sforce.connection.query(query); 

var records = result.getArray(&quot;records&quot;); 

var gc = records[0]; 

if(gc.Advanced_Momentum__c!=gc.Disbursed_Momentum_SF_425__c){
if(gc.Advanced_Momentum__c!=gc.Total_Federal_Expenditure_Final_FFR_SF__c){
window.alert(&apos;Reconciliation amounts do not match. Financial Closeout cannot be performed at this time.&apos;); 
}
}else{
gc.Financial_Closeout_Date__c=new Date();
gc.Date_Funds_Deobligated__c=new Date();
gc.Status__c=&apos;Financial Closeout Complete&apos;;
//alert(gc+ &apos;Recdetails&apos;);
}
var results = sforce.connection.update([records[0]]); 
window.location.reload();</url>
    </webLinks>
