    <validationRules>
        <fullName>Consensus_Results</fullName>
        <active>false</active>
        <errorConditionFormula>OR(AND((Consensus_Approach__c ==&quot;Consensus Scores&quot;),(ISBLANK( Field1__c ))),

AND((Consensus_Approach__c ==&quot;Consensus Scores&quot;),(ISBLANK( Comment_s__c ))))</errorConditionFormula>
        <errorMessage>Enter Number and Comments for Consensus Scores</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Consensus_Results1</fullName>
        <active>false</active>
        <errorConditionFormula>OR(AND((Consensus_Approach__c ==&quot;Consensus Bands&quot;),(ISBLANK(  Response__c ))),
AND((Consensus_Approach__c ==&quot;Consensus Bands&quot;),(ISBLANK(  Comment_s1__c ))))</errorConditionFormula>
        <errorMessage>Enter Response and Comments for Consensus Bands</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>End_Date_greater_than_Start_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Start_Date__c   &gt;=   End_Date__c</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>Panel End Date must be greater than Panel Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Panel_End_Date_with_Stage</fullName>
        <active>true</active>
        <description>The Panel End Date on the Paneling Object must be greater than or equal to the Stage Start Date on the parent Stage, and less than or equal to the Stage End Date on the parent Stage</description>
        <errorConditionFormula>IF( End_Date__c &gt;=  Stage_Number__r.Start_Date__c &amp;&amp; End_Date__c &lt;=  Stage_Number__r.End_Date__c , False, True)</errorConditionFormula>
        <errorDisplayField>End_Date__c</errorDisplayField>
        <errorMessage>End date must be Greater than Stage Review Start date, AND less than Stage Review End date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Stage_Start_Date</fullName>
        <active>true</active>
        <description>Panel Start Date must be greater than or equal to the stage start date</description>
        <errorConditionFormula>IF( Start_Date__c &gt;= Stage_Number__r.Start_Date__c , false, true)</errorConditionFormula>
        <errorDisplayField>Start_Date__c</errorDisplayField>
        <errorMessage>Panel Start Date must be greater than or equal to Stage Start date</errorMessage>
    </validationRules>
