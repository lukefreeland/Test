    <validationRules>
        <fullName>FAPIIS_Review_Comments</fullName>
        <active>true</active>
        <description>As per req #2790</description>
        <errorConditionFormula>OR(
AND(ISPICKVAL( Mitigating_Circumstances__c , &quot;Yes&quot;),(ISBLANK( FAPIIS_Review_Comments__c ))),
AND(
(ISPICKVAL( Satisfactory_Record_Executing_Programs__c , &quot;No&quot;)),
(ISPICKVAL( Demonstrates_Ethics_Integrity__c , &quot;No&quot;)), 
(ISPICKVAL( Mitigating_Circumstances__c , &quot;No&quot;)), 
(ISBLANK( FAPIIS_Review_Comments__c ))))</errorConditionFormula>
        <errorDisplayField>FAPIIS_Review_Comments__c</errorDisplayField>
        <errorMessage>Enter Comments</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sub_Application_Due_Date</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Sub_Application_Due_Date__c)),(Sub_Application_Due_Date__c &gt;  NOFO_RFP__r.Applications_due_date__c))</errorConditionFormula>
        <errorDisplayField>Sub_Application_Due_Date__c</errorDisplayField>
        <errorMessage>The date entered must be at least one day earlier than the &quot;Application Due Date&quot; on the NOFO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sub_Concept_Paper_Due_Date1</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT(ISBLANK(Sub_Application_Due_Date__c)), NOT(ISBLANK(Sub_Concept_Paper_Due_Date__c)),
Sub_Concept_Paper_Due_Date__c  &gt;  Sub_Application_Due_Date__c)</errorConditionFormula>
        <errorDisplayField>Sub_Concept_Paper_Due_Date__c</errorDisplayField>
        <errorMessage>Sub-Concept Paper Due Date must be less than the Sub-Application Due Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sub_Concept_Paper_Due_Date_2</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(ISBLANK(Sub_Concept_Paper_Due_Date__c)) &amp;&amp; ISPICKVAL(NOFO_RFP__r.Allow_Concept_Paper__c, &apos;Yes&apos;) &amp;&amp;   Sub_Concept_Paper_Due_Date__c &gt;  NOFO_RFP__r.Concept_Paper_Due_Date__c</errorConditionFormula>
        <errorDisplayField>Sub_Concept_Paper_Due_Date__c</errorDisplayField>
        <errorMessage>The date entered must be at least one day earlier than the &quot;Concept Paper Due Date&quot; on the NOFO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sub_concept_paper_due_date_rule</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(NOFO_RFP__r.Allow_Concept_Paper__c, &apos;Yes&apos;) &amp;&amp;  ISNULL(Sub_Concept_Paper_Due_Date__c )</errorConditionFormula>
        <errorDisplayField>Sub_Concept_Paper_Due_Date__c</errorDisplayField>
        <errorMessage>The Sub-Concept Paper Due Date can only be entered if a Concept Paper is required on the NOFO</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>if_no_explain_applicant_deliquent_NA</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Applicant_Delinquent_on_any_Federal_Debt__c , &apos;No&apos;)&amp;&amp; NOT(ISBLANK(If_yes_please_explain_app_delinque__c))</errorConditionFormula>
        <errorDisplayField>Applicant_Delinquent_on_any_Federal_Debt__c</errorDisplayField>
        <errorMessage>Entries can only be made if &quot; Applicant Delinquent on any Federal Debt&quot; is Yes.&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>if_no_explain_eo_NA</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( EO_12372_Process__c , &apos;No&apos;) &amp;&amp; NOT(ISBLANK(If_yes_please_explain__c))</errorConditionFormula>
        <errorDisplayField>EO_12372_Process__c</errorDisplayField>
        <errorMessage>Entries can only be made if &quot;EO 12372 Process&quot; is Yes.&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>if_yes_explain_Delinquent</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Applicant_Delinquent_on_any_Federal_Debt__c , &apos;Yes&apos;)&amp;&amp; ISBLANK(If_yes_please_explain_app_delinque__c)</errorConditionFormula>
        <errorDisplayField>If_yes_please_explain_app_delinque__c</errorDisplayField>
        <errorMessage>An entry is required is Applicant Delinquent on any Federal Debt is Yes.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>if_yes_explain_eo</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( EO_12372_Process__c , &apos;Yes&apos;) &amp;&amp; ISBLANK(If_yes_please_explain__c)</errorConditionFormula>
        <errorDisplayField>If_yes_please_explain__c</errorDisplayField>
        <errorMessage>An entry is required is EO 12372 Process is Yes.</errorMessage>
    </validationRules>
