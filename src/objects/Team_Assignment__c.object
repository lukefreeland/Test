    <validationRules>
        <fullName>Actual_End_Date_Required</fullName>
        <active>true</active>
        <description>Actual End Date Required for Cancellation of Team Assignment.</description>
        <errorConditionFormula>AND(Cancelled__c,ISNULL(Actual_End_Date__c))</errorConditionFormula>
        <errorDisplayField>Actual_End_Date__c</errorDisplayField>
        <errorMessage>Please enter Actual End Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campus_Arrival_Date_1</fullName>
        <active>true</active>
        <description>Date selected must be greater than or equal to the &quot;Project Departure Date&quot;</description>
        <errorConditionFormula>Campus_Arrival_Date__c &lt;   Project_Departure_Date__c</errorConditionFormula>
        <errorDisplayField>Campus_Arrival_Date__c</errorDisplayField>
        <errorMessage>Date selected must be greater than or equal to the &quot;Project Departure Date&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campus_Arrival_Date_2</fullName>
        <active>true</active>
        <description>Date selected less than or equal to “Agreement End Date” on the award record.</description>
        <errorConditionFormula>Campus_Arrival_Date__c &gt; Award__r.Agreement_Period_End_Date__c</errorConditionFormula>
        <errorDisplayField>Campus_Arrival_Date__c</errorDisplayField>
        <errorMessage>Date should be less than or equal to “Agreement End Date” on the award record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Campus_Departure_Date</fullName>
        <active>true</active>
        <description>Date selected must be greater than or equal to the “Agreement Start Date” on the award record.</description>
        <errorConditionFormula>Campus_Departure_Date__c  &lt; Award__r.Agreement_Period_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Campus_Departure_Date__c</errorDisplayField>
        <errorMessage>Date selected must be greater than or equal to the “Agreement Start Date” on the award record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cancellation_Justification</fullName>
        <active>true</active>
        <description>Cancellation Justification required for Cancellation of Team Assignment.</description>
        <errorConditionFormula>AND(Cancelled__c,ISPICKVAL(Cancellation_Justification__c,&quot;&quot;))</errorConditionFormula>
        <errorDisplayField>Cancellation_Justification__c</errorDisplayField>
        <errorMessage>Please enter Cancellation Justification.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>No_Transition_Campus_Arrival_Date</fullName>
        <active>true</active>
        <description>If the No Transition checkbox is checked, a Campus Arrival Date should not be entered.</description>
        <errorConditionFormula>AND(No_Transition__c = True,   NOT(ISNULL(Campus_Arrival_Date__c) ))</errorConditionFormula>
        <errorDisplayField>Campus_Arrival_Date__c</errorDisplayField>
        <errorMessage>Campus Arrival Date should not be entered if No Transition is checked.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Arrival_Date</fullName>
        <active>true</active>
        <description>Date selected must be greater than or equal to the “Campus Departure Date ” for the team.</description>
        <errorConditionFormula>Projected_Start_Date__c &lt;  Campus_Departure_Date__c</errorConditionFormula>
        <errorDisplayField>Project_Arrival_Date__c</errorDisplayField>
        <errorMessage>Date must be Greater than or equal to Campus Departure Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Arrival_Date_2</fullName>
        <active>true</active>
        <description>Date selected must be greater than or equal to the “Team Departure Date to the Project ” for the team.</description>
        <errorConditionFormula>Projected_Start_Date__c &lt;   Team_Departure_Date_to_the_Project__c</errorConditionFormula>
        <errorDisplayField>Projected_Start_Date__c</errorDisplayField>
        <errorMessage>Date must be Greater than or equal to Team Departure Date to the Project</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Departure_Date_1</fullName>
        <active>true</active>
        <description>Date selected must be greater than or equal to the &quot;Agreement Start Date&quot;.</description>
        <errorConditionFormula>Project_Departure_Date__c &lt;  Award__r.Project_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Project_Departure_Date__c</errorDisplayField>
        <errorMessage>Date must be Greater than or equal to Agreement Start Date on Award record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Departure_Date_2</fullName>
        <active>true</active>
        <description>Date selected must be less than or equal to the &quot;Agreement End Date&quot;.</description>
        <errorConditionFormula>AND( No_Transition__c = True, 
Project_Departure_Date__c &gt;  Award__r.Agreement_Period_End_Date__c)</errorConditionFormula>
        <errorDisplayField>Project_Departure_Date__c</errorDisplayField>
        <errorMessage>Date must be Less than or equal to Agreement End Date on Award record.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Project_Departure_Date_3</fullName>
        <active>true</active>
        <description>Date entered in the Project Departure Date must be greater than or equal to the Project Arrival Date.</description>
        <errorConditionFormula>Project_Departure_Date__c &lt; Projected_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Project_Departure_Date__c</errorDisplayField>
        <errorMessage>Project Departure Date must be greater than or equal to the Projected Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Projected_End_Date</fullName>
        <active>true</active>
        <description>Projected End Date must be greater than or equal to the “Campus Arrival Date.” 
If the “No Transition” checkbox is checked, the date entered must be greater than or equal to the “Project Departure Date.”</description>
        <errorConditionFormula>IF ( No_Transition__c == False, Projected_End_Date__c &lt;  Campus_Arrival_Date__c, Projected_End_Date__c &lt;  Project_Departure_Date__c )</errorConditionFormula>
        <errorDisplayField>Projected_End_Date__c</errorDisplayField>
        <errorMessage>Projected End Date must be greater than or equal to the “Campus Arrival Date.” 
If the “No Transition” checkbox is checked, the date entered must be greater than or equal to the “Project Departure Date.”</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Team_Departure_Date_to_the_Project</fullName>
        <active>true</active>
        <description>Team Departure Date to the Project date selected must be greater than or equal to the “Agreement Start Date” on the award record.</description>
        <errorConditionFormula>Team_Departure_Date_to_the_Project__c &lt; Award__r.Agreement_Period_Start_Date__c</errorConditionFormula>
        <errorDisplayField>Team_Departure_Date_to_the_Project__c</errorDisplayField>
        <errorMessage>Team Departure Date to the Project should be Greater than Agreement Period Start Date.</errorMessage>
    </validationRules>
</CustomObject>
