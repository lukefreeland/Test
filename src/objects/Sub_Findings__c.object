    <validationRules>
        <fullName>Corrective_Action_Taken_must_be_entered</fullName>
        <active>false</active>
        <description>Corrective Action Taken must be entered if a finding number is entered</description>
        <errorConditionFormula>AND(NOT(ISBLANK(Finding_Number__c)) 
, LEN(Corrective_Action_Taken__c) =0)</errorConditionFormula>
        <errorDisplayField>Corrective_Action_Taken__c</errorDisplayField>
        <errorMessage>Corrective Action Taken must be entered if a finding number is entered</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Number_Validation_on_Finding_Number</fullName>
        <active>true</active>
        <errorConditionFormula>NOT( 
OR( 
LEN (Finding_Number__c) = 0, 
REGEX(Finding_Number__c, &quot;[0-9]{2}&quot;) 
))</errorConditionFormula>
        <errorDisplayField>Finding_Number__c</errorDisplayField>
        <errorMessage>Must enter only two numbers between {0-9}</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Other_Findings_Required</fullName>
        <active>true</active>
        <description>If “Other” is selected as a finding, a response must be entered in the “Other Findings” textbox.</description>
        <errorConditionFormula>AND( 
INCLUDES (Finding_Category__c , &quot;Other&quot;), 
ISBLANK(Other_Findings__c) 
)</errorConditionFormula>
        <errorDisplayField>Other_Findings__c</errorDisplayField>
        <errorMessage>If “Other” is selected as a finding, a response must be entered in the “Other Findings” textbox.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Sub_Finding_Number_Required</fullName>
        <active>true</active>
        <description>A finding number must be entered if a Finding Category is selected.</description>
        <errorConditionFormula>AND(NOT(ISBLANK(Finding_Category__c)),ISBLANK( Finding_Number__c ))</errorConditionFormula>
        <errorDisplayField>Finding_Number__c</errorDisplayField>
        <errorMessage>A finding number must be entered if a Finding Category is selected.</errorMessage>
    </validationRules>
